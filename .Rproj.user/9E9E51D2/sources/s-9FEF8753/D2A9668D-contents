library(fs)
library(tidyverse)
library(readxl)

paths <- dir_ls("./results", glob = "*.xlsx")
paths

map(read_excel, path = paths)

myfiles <- list.files(path = "./results", pattern = "*.xlsx")



my_data <- map_df(myfiles, read_xlsx)

library(readr)
files <- list.files(path = "./results", pattern = "*.xlsx", full.names = T)

tbl <- map_dfr(files, read_xlsx)
